"RuleSolver": ""
}

[application]

config/name="ld51"
run/main_scene="res://scenes/leo_test_scene.tscn"
config/icon="res://icon.png"

[display]

window/size/width=400
window/size/height=200
window/size/test_width=400
window/size/test_height=200
window/stretch/mode="viewport"
